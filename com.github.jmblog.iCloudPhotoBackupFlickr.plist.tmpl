<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.github.jmblog.iCloudPhotoBackupFlickr</string>
	<key>ProgramArguments</key>
	<array>
		<string><%= node_path %></string>
		<string><%= backupjs_path %></string>
	</array>
	<key>WatchPaths</key>
	<array>
		<string><%= home_dir %>/Library/Application Support/iLifeAssetManagement/assets/sub</string>
	</array>
	<key>StartInterval</key>
	<integer>60</integer>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
